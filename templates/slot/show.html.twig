{% extends 'base.html.twig' %}

{% block title %}Slot{% endblock %}

{% block body %}
    <h1>Slot</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ slot.id }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ slot.dateDebut ? slot.dateDebut|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateEnd</th>
                <td>{{ slot.dateEnd ? slot.dateEnd|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_slot_index') }}">back to list</a>

    <a href="{{ path('app_slot_edit', {'id': slot.id}) }}">edit</a>

    {{ include('slot/_delete_form.html.twig') }}
{% endblock %}
